# INATrace - DEV environment (.env template)
IMAGE_NAME=ghcr.io/atijaguar-ec/backend-inatrace
TAG=develop

# Database
DATABASE_NAME=inatrace
DB_ROOT_PASSWORD=root
DATASOURCE_USERNAME=inatrace
DATASOURCE_PASSWORD=inatrace

# Containers and ports
CONTAINER_NAME_BE=inatrace-be-dev
CONTAINER_NAME_DB=inatrace-mysql-dev
HOST_PORT_BE=8080
HOST_PORT_DB=3306

# Volumes (host paths)
DB_VOLUME=/opt/inatrace/mysql
FILE_STORAGE_ROOT=/opt/inatrace/uploads
# IMPORT_PATH uses container's built-in files (countries.csv for migrations)
IMPORT_PATH=/app/import/
DOCUMENTS_ROOT=/opt/inatrace/documents
FILE_STORAGE_ROOT_VOL=/opt/inatrace/volumes/uploads
# No volume for import - use container's built-in files
IMPORT_PATH_VOL=/app/import
DOCUMENTS_ROOT_VOL=/opt/inatrace/volumes/documents

# Integrations
REQUESTLOG_TOKEN=dev-requestlog-CHANGE_ME
EXCHANGERATE_APIKEY=
FE_URL=https://inatrace.atijaguar.com/
FE_CONFIRM_URL=https://inatrace.atijaguar.com/confirm-email/
FE_RESET_URL=https://inatrace.atijaguar.com/reset-password/

# Mail
SPRING_MAIL_HOST=mail.atijaguar.com
SPRING_MAIL_PORT=465
SPRING_MAIL_USERNAME=desarrollo@atijaguar.com
SPRING_MAIL_PASSWORD=CHANGE_ME
SPRING_MAIL_SMTP_AUTH=true
SPRING_MAIL_SMTP_SSL_ENABLE=true
SPRING_MAIL_STARTTLS_ENABLE=false
SPRING_MAIL_SMTP_CONNECTION_TIMEOUT=5000
SPRING_MAIL_SMTP_TIMEOUT=3000
SPRING_MAIL_SMTP_WRITETIMEOUT=5000
SPRING_MAIL_DEBUG=false
INATRACE_MAIL_TEMPLATE_FROM=no-reply@inatrace.com
INATRACE_MAIL_REDIRECT=
INATRACE_MAIL_SENDING_ENABLED=false

# AgStack (optional in dev)
INATRACE_AGSTACK_EMAIL=
INATRACE_AGSTACK_PASSWORD=
