# INATrace - FORTALEZA TEST (.env template for Jenkins/CEDIA)
IMAGE_NAME=ghcr.io/atijaguar-ec/backend-inatrace
TAG=test-CHANGE_SHA

# Database
DATABASE_NAME=inatrace_test_fortaleza
DB_ROOT_PASSWORD=CHANGE_ME
DATASOURCE_USERNAME=inatrace_test_fortaleza
DATASOURCE_PASSWORD=CHANGE_ME

# Volumes (host paths)
DB_VOLUME=/opt/inatrace/mysql
FILE_STORAGE_ROOT=/opt/inatrace/uploads
IMPORT_PATH=/opt/inatrace/import
DOCUMENTS_ROOT=/opt/inatrace/documents
FILE_STORAGE_ROOT_VOL=/opt/inatrace/uploads
IMPORT_PATH_VOL=/opt/inatrace/imports
DOCUMENTS_ROOT_VOL=/opt/inatrace/documents

# Integrations
REQUESTLOG_TOKEN=placeholder-requestlog-CHANGE_ME
EXCHANGERATE_APIKEY=dummy-test-key
FE_URL=https://testinatrace.espam.edu.ec
FE_CONFIRM_URL=https://testinatrace.espam.edu.ec/confirm-email/
FE_RESET_URL=https://testinatrace.espam.edu.ec/reset-password/

# Mail
SPRING_MAIL_HOST=mail.atijaguar.com
SPRING_MAIL_PORT=465
SPRING_MAIL_USERNAME=desarrollo@atijaguar.com
SPRING_MAIL_PASSWORD=CHANGE_ME
SPRING_MAIL_SMTP_AUTH=true
SPRING_MAIL_SMTP_SSL_ENABLE=true
SPRING_MAIL_STARTTLS_ENABLE=false
SPRING_MAIL_SMTP_CONNECTION_TIMEOUT=5000
SPRING_MAIL_SMTP_TIMEOUT=3000
SPRING_MAIL_SMTP_WRITETIMEOUT=5000
SPRING_MAIL_DEBUG=false
INATRACE_MAIL_TEMPLATE_FROM=no-reply@inatrace.com
INATRACE_MAIL_REDIRECT=desarrollo@atijaguar.com
INATRACE_MAIL_SENDING_ENABLED=true

# AgStack (optional: set if organization uses AgStack)
INATRACE_AGSTACK_EMAIL=
INATRACE_AGSTACK_PASSWORD=
